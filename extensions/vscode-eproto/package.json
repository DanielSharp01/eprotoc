{
  "name": "vscode-eproto",
  "displayName": "EProto",
  "description": "EProto language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "eproto",
      "aliases": ["EProto", "eproto"],
      "extensions": [".eproto"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "eproto",
      "scopeName": "source.eproto",
      "path": "./syntaxes/eproto.tmLanguage.json"
    }]
  }
}
